jessyInkSetupDescription="JessyInk : Web based presentation, slides (Inkscape plugin)"
jessyInkSetupStatus()
{
  return $false
}
jessyInkSetup()
{
  cd
  autoInstall bzr bzr
  autoInstall inkscape inkscape
  bzr branch lp:jessyink
  $udo cp -r jessyink/inkscapeExtensions/* /usr/share/inkscape/extensions/
  cd /usr/share/inkscape/extensions/ || exit 1
  $udo chmod 755 jessy*py
  $udo chmod 755 jessyInk.js
  $udo chmod 644 jessy*inx
}
